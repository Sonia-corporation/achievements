{
  "name": "@sonia-corporation/achievements",
  "version": "0.0.0",
  "author": {
    "email": "geoffrey.testelin@gmail.com",
    "name": "Geoffrey 'C0ZEN' Testelin",
    "url": "https://github.com/C0ZEN"
  },
  "contributors": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "description": "Check and manage the achievements of your IT team.",
  "homepage": "https://github.com/Sonia-corporation/achievements",
  "maintainers": [
    {
      "email": "geoffrey.testelin@gmail.com",
      "name": "Geoffrey 'C0ZEN' Testelin",
      "url": "https://github.com/C0ZEN"
    }
  ],
  "license": "MIT",
  "keywords": [
    "achievements",
    "it",
    "team"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Sonia-corporation/achievements.git"
  },
  "bugs": {
    "url": "https://github.com/Sonia-corporation/achievements/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./scripts/commitizen.js"
    }
  },
  "akitaCli": {
    "template": "ts"
  },
  "engines": {
    "node": "14.17.x",
    "npm": "7"
  },
  "private": false,
  "files": [
    "dist/*",
    "CHANGELOG.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "\n# RUN SCRIPTS:": "",
    "start": "cross-env NODE_ENV=development ts-node-dev --debounce --clear --cache-directory dist-local-dev --respawn --transpile-only .",
    "\n# BUILD SCRIPTS:": "",
    "build": "",
    "\n# LINT SCRIPTS:": "",
    "lint": "eslint --fix --fix-type suggestion,layout,problem --cache --cache-location .eslintcache --quiet **/*.{js,ts}",
    "lint:hook": "eslint --fix --fix-type suggestion,layout,problem --cache --cache-location .eslintcache --quiet",
    "lint:ci": "eslint --quiet **/*.{js,ts}",
    "prettier": "prettier --write --ignore-unknown **/*.{md,json,yml,html}",
    "prettier:hook": "prettier --write --ignore-unknown --loglevel warn",
    "\n# UTILS SCRIPTS:": "",
    "cz": "git add . && git-cz && npm run git:push",
    "semver": "semantic-release",
    "clean:dist": "rimraf dist",
    "commit": "npm run cz",
    "git:push": "git push-upstream",
    "madge:circular-dependencies-check": "madge ./src/ --circular --extensions ts,js --image ./dist/circular-dependencies-graph.svg --warning",
    "madge:image": "madge ./src/ --extensions ts,js --image ./dist/graph.svg --warning",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit-msg": "cross-env LOGGING_LEVEL=debug cz-customizable-ghooks",
    "post-commit": "git update-index --again",
    "install-git-hooks": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "devDependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@sonia-corporation/conventional-changelog": "1.0.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "all-contributors-cli": "6.20.0",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "cz-customizable-ghooks": "2.0.0",
    "eslint": "7.27.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-recommended": "4.1.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-jsdoc": "35.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-progress": "0.0.1",
    "eslint-plugin-rxjs": "3.3.1",
    "eslint-plugin-typescript-sort-keys": "1.6.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "madge": "4.0.2",
    "node-notifier": "10.0.0",
    "prettier": "2.2.1",
    "semantic-release": "17.4.3",
    "shx": "0.3.3",
    "typescript": "4.3.2"
  }
}
